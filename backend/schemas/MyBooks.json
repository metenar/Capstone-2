{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "required": [
        "book_id",
        "current_status"
    ],
    "properties": {
        "username": {
            "type": "string"
        },
        "book_id": {
            "type": "string"
        },
        "current_status": {
            "type": "string"
        },
        "rating": {
            "type": "string"
        },
        "finished_date": {
            "type": "string",
            "format": "date"
        },
        "progress": {
            "type": "number"
        }
    },
    "additionalProperties": true
}